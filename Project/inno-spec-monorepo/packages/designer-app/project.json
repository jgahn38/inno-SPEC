{
  "name": "designer-app",
  "sourceRoot": "packages/designer-app/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "designer-app:build",
        "configFile": "packages/designer-app/vite.config.ts",
        "port": 5173,
        "host": "localhost",
        "root": "packages/designer-app"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/designer-app/dist",
        "configFile": "packages/designer-app/vite.config.ts",
        "root": "packages/designer-app"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "designer-app:build"
      }
    }
  },
  "tags": []
}
